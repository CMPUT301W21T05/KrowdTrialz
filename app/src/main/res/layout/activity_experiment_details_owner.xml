<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".ui.ExperimentDetailsOwnerActivity">

    <EditText
        android:id="@+id/description_edit_detail_screen_owner"
        android:layout_width="335dp"
        android:layout_height="50dp"
        android:layout_marginTop="5dp"
        android:layout_marginEnd="4dp"
        android:layout_marginRight="4dp"
        android:hint="Description"
        android:inputType="textPersonName"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

    <TextView
        android:id="@+id/region_detail_screen_owner"
        android:layout_width="335dp"
        android:layout_height="35dp"
        android:layout_marginTop="5dp"
        android:text="Region"
        app:layout_constraintStart_toStartOf="@+id/description_edit_detail_screen_owner"
        app:layout_constraintTop_toBottomOf="@+id/description_edit_detail_screen_owner" />

    <TextView
        android:id="@+id/minimum_trial_text_screen_owner"
        android:layout_width="wrap_content"
        android:layout_height="45dp"
        android:layout_marginTop="5dp"
        android:text="Minimum trials: "
        app:layout_constraintStart_toStartOf="@+id/region_detail_screen_owner"
        app:layout_constraintTop_toBottomOf="@+id/region_detail_screen_owner" />

    <EditText
        android:id="@+id/minimum_trial_edit_screen_owner"
        android:layout_width="150dp"
        android:layout_height="45dp"
        android:layout_marginLeft="30dp"
        android:layout_marginTop="5dp"
        android:hint="Enter a number"
        android:inputType="number"
        app:layout_constraintStart_toEndOf="@id/minimum_trial_text_screen_owner"
        app:layout_constraintTop_toBottomOf="@+id/region_detail_screen_owner"/>

    <Button
        android:id="@+id/end_experiment_button_detail_screen_owner"
        android:layout_width="168dp"
        android:layout_height="35dp"
        android:layout_marginTop="5dp"
        android:text="End Experiment"
        app:layout_constraintStart_toStartOf="@+id/description_edit_detail_screen_owner"
        app:layout_constraintTop_toBottomOf="@+id/minimum_trial_text_screen_owner" />

    <Button
        android:id="@+id/unpublish_button_screen_owner"
        android:layout_width="140dp"
        android:layout_height="35dp"
        android:layout_marginTop="5dp"
        android:layout_marginLeft="30dp"
        android:text="Unpublish"
        app:layout_constraintStart_toEndOf="@+id/end_experiment_button_detail_screen_owner"
        app:layout_constraintTop_toBottomOf="@+id/minimum_trial_text_screen_owner"
        android:layout_marginStart="30dp" />

    <Button
        android:id="@+id/view_contributors_button_screen_owner"
        android:layout_width="335dp"
        android:layout_height="50dp"
        android:layout_marginTop="5dp"
        android:text="View Contributors"
        app:layout_constraintStart_toStartOf="@+id/end_experiment_button_detail_screen_owner"
        app:layout_constraintTop_toBottomOf="@+id/end_experiment_button_detail_screen_owner"
        />

    <Button
        android:id="@+id/subscribe_button_detail_screen_owner"
        android:layout_width="335dp"
        android:layout_height="50dp"
        android:layout_marginTop="5dp"
        android:text="Subscribe"
        app:layout_constraintStart_toStartOf="@+id/view_contributors_button_screen_owner"
        app:layout_constraintTop_toBottomOf="@+id/view_contributors_button_screen_owner" />

    <Button
        android:id="@+id/add_trials_button_detail_screen_owner"
        android:layout_width="335dp"
        android:layout_height="50dp"
        android:layout_marginTop="5dp"
        android:text="Add trials"
        app:layout_constraintStart_toStartOf="@+id/subscribe_button_detail_screen_owner"
        app:layout_constraintTop_toBottomOf="@+id/subscribe_button_detail_screen_owner" />

    <TextView
        android:id="@+id/mean_detail_screen_owner"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginTop="15dp"
        android:layout_marginLeft="75dp"
        android:text="1.23"
        app:layout_constraintStart_toStartOf="@+id/add_trials_button_detail_screen_owner"
        app:layout_constraintTop_toBottomOf="@+id/add_trials_button_detail_screen_owner"
        android:layout_marginStart="75dp" />

    <TextView
        android:id="@+id/st_dev_detail_screen_owner"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginTop="15dp"
        android:layout_marginRight="75dp"
        android:text="0.98"
        app:layout_constraintEnd_toEndOf="@+id/add_trials_button_detail_screen_owner"
        app:layout_constraintTop_toBottomOf="@+id/add_trials_button_detail_screen_owner"
        android:layout_marginEnd="75dp" />

    <TextView
        android:id="@+id/mean_text_detail_screen_owner"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginTop="5dp"
        android:layout_marginLeft="75dp"
        android:text="mean"
        app:layout_constraintStart_toStartOf="@+id/add_trials_button_detail_screen_owner"
        app:layout_constraintTop_toBottomOf="@+id/mean_detail_screen_owner"
        android:layout_marginStart="75dp" />

    <TextView
        android:id="@+id/st_dev_text_detail_screen_owner"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginTop="5dp"
        android:layout_marginRight="75dp"
        android:text="stdev"
        app:layout_constraintEnd_toEndOf="@+id/add_trials_button_detail_screen_owner"
        app:layout_constraintTop_toBottomOf="@+id/st_dev_detail_screen_owner"
        android:layout_marginEnd="75dp" />

    <TextView
        android:id="@+id/first_experiment_detail_screen_owner"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginTop="5dp"
        android:text="0.00"
        app:layout_constraintStart_toStartOf="@+id/add_trials_button_detail_screen_owner"
        app:layout_constraintTop_toBottomOf="@+id/st_dev_text_detail_screen_owner" />

    <TextView
        android:id="@+id/second_experiment_detail_screen_owner"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginTop="5dp"
        android:layout_marginLeft="75dp"
        android:text="0.00"
        app:layout_constraintStart_toStartOf="@+id/first_experiment_detail_screen_owner"
        app:layout_constraintTop_toBottomOf="@+id/st_dev_text_detail_screen_owner"
        android:layout_marginStart="75dp" />

    <TextView
        android:id="@+id/third_experiment_detail_screen_owner"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginTop="5dp"
        android:layout_marginLeft="75dp"
        android:text="0.00"
        app:layout_constraintStart_toStartOf="@+id/second_experiment_detail_screen_owner"
        app:layout_constraintTop_toBottomOf="@+id/st_dev_text_detail_screen_owner"
        android:layout_marginStart="75dp" />

    <TextView
        android:id="@+id/fourth_experiment_detail_screen_owner"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginTop="5dp"
        android:layout_marginLeft="75dp"
        android:text="0.00"
        app:layout_constraintStart_toStartOf="@+id/third_experiment_detail_screen_owner"
        app:layout_constraintTop_toBottomOf="@+id/st_dev_text_detail_screen_owner"
        android:layout_marginStart="75dp" />

    <TextView
        android:id="@+id/fifth_experiment_detail_screen_owner"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginTop="5dp"
        android:text="0.00"
        app:layout_constraintEnd_toEndOf="@+id/add_trials_button_detail_screen_owner"
        app:layout_constraintTop_toBottomOf="@+id/st_dev_text_detail_screen_owner" />

    <TextView
        android:id="@+id/first_experiment_text_detail_screen_owner"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginTop="5dp"
        android:text="q0"
        app:layout_constraintStart_toStartOf="@+id/add_trials_button_detail_screen_owner"
        app:layout_constraintTop_toBottomOf="@+id/first_experiment_detail_screen_owner" />

    <TextView
        android:id="@+id/second_experiment_text_detail_screen_owner"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginTop="5dp"
        android:layout_marginLeft="75dp"
        android:text="q1"
        app:layout_constraintStart_toStartOf="@+id/first_experiment_detail_screen_owner"
        app:layout_constraintTop_toBottomOf="@+id/first_experiment_detail_screen_owner"
        android:layout_marginStart="75dp" />

    <TextView
        android:id="@+id/third_experiment_text_detail_screen_owner"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginTop="5dp"
        android:layout_marginLeft="75dp"
        android:text="q2"
        app:layout_constraintStart_toStartOf="@+id/second_experiment_detail_screen_owner"
        app:layout_constraintTop_toBottomOf="@+id/first_experiment_detail_screen_owner"
        android:layout_marginStart="75dp" />

    <TextView
        android:id="@+id/fourth_experiment_text_detail_screen_owner"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginTop="5dp"
        android:layout_marginLeft="75dp"
        android:text="q3"
        app:layout_constraintStart_toStartOf="@+id/third_experiment_detail_screen_owner"
        app:layout_constraintTop_toBottomOf="@+id/first_experiment_detail_screen_owner"
        android:layout_marginStart="75dp" />

    <TextView
        android:id="@+id/fifth_experiment_text_detail_screen_owner"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginTop="5dp"
        android:text="q4"
        app:layout_constraintEnd_toEndOf="@+id/add_trials_button_detail_screen_owner"
        app:layout_constraintTop_toBottomOf="@+id/first_experiment_detail_screen_owner" />

    <TextView
        android:id="@+id/median_text_detail_screen_owner"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginTop="5dp"
        android:text="median"
        app:layout_constraintEnd_toEndOf="@+id/third_experiment_detail_screen_owner"
        app:layout_constraintTop_toBottomOf="@+id/first_experiment_text_detail_screen_owner" />

    <Button
        android:id="@+id/view_map_button_detail_screen_owner"
        android:layout_width="335dp"
        android:layout_height="40dp"
        android:layout_marginTop="5dp"
        android:text="view map"
        app:layout_constraintStart_toStartOf="@+id/description_edit_detail_screen_owner"
        app:layout_constraintTop_toBottomOf="@+id/median_text_detail_screen_owner" />

    <Button
        android:id="@+id/view_QnA_button_detail_screen_owner"
        android:layout_width="335dp"
        android:layout_height="40dp"
        android:layout_marginTop="5dp"
        android:text="View Q&amp;A"
        app:layout_constraintStart_toStartOf="@+id/description_edit_detail_screen_owner"
        app:layout_constraintTop_toBottomOf="@+id/view_map_button_detail_screen_owner" />

    <com.github.mikephil.charting.charts.BarChart
        android:id="@+id/histogram_owner"
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:layout_marginTop="20dp"
        app:layout_constraintTop_toBottomOf="@+id/view_QnA_button_detail_screen_owner" />

    <com.github.mikephil.charting.charts.ScatterChart
        android:id="@+id/time_plot_owner"
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:layout_marginTop="20dp"
        app:layout_constraintTop_toBottomOf="@+id/histogram_non_owner" />


    <com.google.android.material.bottomnavigation.BottomNavigationView
        android:id="@+id/nav_view"
        android:layout_width="0dp"
        android:layout_height="wrap_content"
        android:layout_marginStart="0dp"
        android:layout_marginEnd="0dp"
        android:background="?android:attr/windowBackground"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintLeft_toLeftOf="parent"
        app:layout_constraintRight_toRightOf="parent"
        app:menu="@menu/bottom_nav_menu" />

    <fragment
        android:id="@+id/nav_host_fragment"
        android:name="androidx.navigation.fragment.NavHostFragment"
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        app:defaultNavHost="true"
        app:layout_constraintBottom_toTopOf="@id/nav_view"
        app:layout_constraintLeft_toLeftOf="parent"
        app:layout_constraintRight_toRightOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:navGraph="@navigation/mobile_navigation" />

</androidx.constraintlayout.widget.ConstraintLayout>