<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".ui.ExperimentDetailsNonOwnerActivity">

    <TextView
        android:id="@+id/owner_username_detail_screen_non_owner"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginTop="30dp"
        android:layout_marginRight="230dp"
        android:text="Owner Username"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        android:layout_marginEnd="230dp" />

    <TextView
        android:id="@+id/description_detail_screen_non_owner"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginTop="5dp"
        android:layout_marginRight="70dp"
        android:text="Description"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.877"
        app:layout_constraintTop_toTopOf="parent"
        android:layout_marginEnd="70dp" />

    <TextView
        android:id="@+id/status_detail_screen_non_owner"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginTop="5dp"
        android:text="Status"
        app:layout_constraintStart_toStartOf="@+id/description_detail_screen_non_owner"
        app:layout_constraintTop_toBottomOf="@+id/description_detail_screen_non_owner" />

    <TextView
        android:id="@+id/region_detail_screen_non_owner"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginTop="5dp"
        android:text="Region"
        app:layout_constraintStart_toStartOf="@+id/status_detail_screen_non_owner"
        app:layout_constraintTop_toBottomOf="@+id/status_detail_screen_non_owner"
        />

    <Button
        android:id="@+id/subscribe_button_detail_screen_non_owner"
        android:layout_width="335dp"
        android:layout_height="50dp"
        android:layout_marginTop="5dp"
        android:text="Subscribe"
        app:layout_constraintStart_toStartOf="@+id/owner_username_detail_screen_non_owner"
        app:layout_constraintTop_toBottomOf="@+id/region_detail_screen_non_owner" />

    <Button
        android:id="@+id/add_trials_button_detail_screen_non_owner"
        android:layout_width="335dp"
        android:layout_height="50dp"
        android:layout_marginTop="5dp"
        android:text="ADD Trials"
        app:layout_constraintStart_toStartOf="@+id/subscribe_button_detail_screen_non_owner"
        app:layout_constraintTop_toBottomOf="@+id/subscribe_button_detail_screen_non_owner" />

    <TextView
        android:id="@+id/mean_detail_screen_non_owner"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginTop="15dp"
        android:layout_marginLeft="75dp"
        android:text="1.23"
        app:layout_constraintStart_toStartOf="@+id/add_trials_button_detail_screen_non_owner"
        app:layout_constraintTop_toBottomOf="@+id/add_trials_button_detail_screen_non_owner"
        android:layout_marginStart="75dp" />

    <TextView
        android:id="@+id/st_dev_detail_screen_non_owner"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginTop="15dp"
        android:layout_marginRight="75dp"
        android:text="0.98"
        app:layout_constraintEnd_toEndOf="@+id/add_trials_button_detail_screen_non_owner"
        app:layout_constraintTop_toBottomOf="@+id/add_trials_button_detail_screen_non_owner"
        android:layout_marginEnd="75dp" />

    <TextView
        android:id="@+id/mean_text_detail_screen_non_owner"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginTop="5dp"
        android:layout_marginLeft="75dp"
        android:text="Mean"
        app:layout_constraintStart_toStartOf="@+id/add_trials_button_detail_screen_non_owner"
        app:layout_constraintTop_toBottomOf="@+id/mean_detail_screen_non_owner"
        android:layout_marginStart="75dp" />

    <TextView
        android:id="@+id/st_dev_text_detail_screen_non_owner"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginTop="5dp"
        android:layout_marginRight="75dp"
        android:text="stdev"
        app:layout_constraintEnd_toEndOf="@+id/add_trials_button_detail_screen_non_owner"
        app:layout_constraintTop_toBottomOf="@+id/st_dev_detail_screen_non_owner"
        android:layout_marginEnd="75dp" />

    <TextView
        android:id="@+id/first_experiment_detail_screen_non_owner"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginTop="5dp"
        android:text="0.00"
        app:layout_constraintStart_toStartOf="@+id/add_trials_button_detail_screen_non_owner"
        app:layout_constraintTop_toBottomOf="@+id/st_dev_text_detail_screen_non_owner" />

    <TextView
        android:id="@+id/second_experiment_detail_screen_non_owner"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginTop="5dp"
        android:layout_marginLeft="75dp"
        android:text="0.00"
        app:layout_constraintStart_toStartOf="@+id/first_experiment_detail_screen_non_owner"
        app:layout_constraintTop_toBottomOf="@+id/st_dev_text_detail_screen_non_owner"
        android:layout_marginStart="75dp" />

    <TextView
        android:id="@+id/third_experiment_detail_screen_non_owner"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginTop="5dp"
        android:layout_marginLeft="75dp"
        android:text="0.00"
        app:layout_constraintStart_toStartOf="@+id/second_experiment_detail_screen_non_owner"
        app:layout_constraintTop_toBottomOf="@+id/st_dev_text_detail_screen_non_owner"
        android:layout_marginStart="75dp" />

    <TextView
        android:id="@+id/fourth_experiment_detail_screen_non_owner"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginTop="5dp"
        android:layout_marginLeft="75dp"
        android:text="0.00"
        app:layout_constraintStart_toStartOf="@+id/third_experiment_detail_screen_non_owner"
        app:layout_constraintTop_toBottomOf="@+id/st_dev_text_detail_screen_non_owner"
        android:layout_marginStart="75dp" />

    <TextView
        android:id="@+id/fifth_experiment_detail_screen_non_owner"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginTop="5dp"
        android:text="0.00"
        app:layout_constraintEnd_toEndOf="@+id/add_trials_button_detail_screen_non_owner"
        app:layout_constraintTop_toBottomOf="@+id/st_dev_text_detail_screen_non_owner" />

    <TextView
        android:id="@+id/first_experiment_text_detail_screen_non_owner"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginTop="5dp"
        android:text="q0"
        app:layout_constraintStart_toStartOf="@+id/add_trials_button_detail_screen_non_owner"
        app:layout_constraintTop_toBottomOf="@+id/first_experiment_detail_screen_non_owner" />

    <TextView
        android:id="@+id/second_experiment_text_detail_screen_non_owner"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginTop="5dp"
        android:layout_marginLeft="75dp"
        android:text="q1"
        app:layout_constraintStart_toStartOf="@+id/first_experiment_detail_screen_non_owner"
        app:layout_constraintTop_toBottomOf="@+id/first_experiment_detail_screen_non_owner"
        android:layout_marginStart="75dp" />

    <TextView
        android:id="@+id/third_experiment_text_detail_screen_non_owner"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginTop="5dp"
        android:layout_marginLeft="75dp"
        android:text="q2"
        app:layout_constraintStart_toStartOf="@+id/second_experiment_detail_screen_non_owner"
        app:layout_constraintTop_toBottomOf="@+id/first_experiment_detail_screen_non_owner"
        android:layout_marginStart="75dp" />

    <TextView
        android:id="@+id/fourth_experiment_text_detail_screen_non_owner"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginTop="5dp"
        android:layout_marginLeft="75dp"
        android:text="q3"
        app:layout_constraintStart_toStartOf="@+id/third_experiment_detail_screen_non_owner"
        app:layout_constraintTop_toBottomOf="@+id/first_experiment_detail_screen_non_owner"
        android:layout_marginStart="75dp" />

    <TextView
        android:id="@+id/fifth_experiment_text_detail_screen_non_owner"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginTop="5dp"
        android:text="q4"
        app:layout_constraintEnd_toEndOf="@+id/add_trials_button_detail_screen_non_owner"
        app:layout_constraintTop_toBottomOf="@+id/first_experiment_detail_screen_non_owner" />

    <TextView
        android:id="@+id/median_text_detail_screen_non_owner"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginTop="5dp"
        android:text="median"
        app:layout_constraintEnd_toEndOf="@+id/third_experiment_detail_screen_non_owner"
        app:layout_constraintTop_toBottomOf="@+id/first_experiment_text_detail_screen_non_owner" />

    <Button
        android:id="@+id/view_map_button_detail_screen_non_owner"
        android:layout_width="335dp"
        android:layout_height="40dp"
        android:layout_marginTop="5dp"
        android:text="view map"
        app:layout_constraintStart_toStartOf="@+id/owner_username_detail_screen_non_owner"
        app:layout_constraintTop_toBottomOf="@+id/median_text_detail_screen_non_owner" />

    <Button
        android:id="@+id/view_QnA_button_detail_screen_non_owner"
        android:layout_width="335dp"
        android:layout_height="40dp"
        android:layout_marginTop="5dp"
        android:text="view Q&amp;A"
        app:layout_constraintStart_toStartOf="@+id/owner_username_detail_screen_non_owner"
        app:layout_constraintTop_toBottomOf="@+id/view_map_button_detail_screen_non_owner" />

    <com.github.mikephil.charting.charts.BarChart
        android:id="@+id/histogram_non_owner_barChart"
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:layout_marginTop="20dp"
        app:layout_constraintTop_toBottomOf="@+id/view_QnA_button_detail_screen_non_owner" />

    <com.github.mikephil.charting.charts.ScatterChart
        android:id="@+id/time_plot_non_owner_scatterChart"
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:layout_marginTop="20dp"
        app:layout_constraintTop_toBottomOf="@+id/histogram_non_owner_barChart" />


    <com.google.android.material.bottomnavigation.BottomNavigationView
        android:id="@+id/nav_view"
        android:layout_width="0dp"
        android:layout_height="wrap_content"
        android:layout_marginStart="0dp"
        android:layout_marginEnd="0dp"
        android:background="?android:attr/windowBackground"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintLeft_toLeftOf="parent"
        app:layout_constraintRight_toRightOf="parent"
        app:menu="@menu/bottom_nav_menu" />

    <fragment
        android:id="@+id/nav_host_fragment"
        android:name="androidx.navigation.fragment.NavHostFragment"
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        app:defaultNavHost="true"
        app:layout_constraintBottom_toTopOf="@id/nav_view"
        app:layout_constraintLeft_toLeftOf="parent"
        app:layout_constraintRight_toRightOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:navGraph="@navigation/mobile_navigation" />

</androidx.constraintlayout.widget.ConstraintLayout>